{
	"name": "automated-file-activity",
	"displayName": "automated-file-activity",
	"description": "Automated File Activity for hack any desktop tracker",
	"version": "0.0.1",
	"publisher": "automated-file-activity",
	"engines": {
		"vscode": "^1.25.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.afa.active"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [{
			"command": "extension.afa.active",
			"title": "Active Automated File Activity"
		}],
		"configuration": {
			"type": "object",
			"title": "File Peek extension configuration",
			"properties": {
				"afa.activeLanguages": {
					"type": "array",
					"default": [
						"typescript",
						"javascript",
						"python"
					],
					"items": {
						"type": "string"
					},
					"description": "A list of vscode language names where the extension should be used."
				},
				"afa.searchFileExtensions": {
					"type": "array",
					"default": [".js", ".ts", ".html", ".css", ".scss"],
					"items": {
						"type": "string"
					},
					"description": "A list of extensions that should be tried for finding peeked files.  These are tried in order as further extensions of the potential file name and also as alternative file endings instead of the existing file extension (if available)."
				}
			}
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install"
	},
	"devDependencies": {
		"@types/node": "^8.10.25",
		"globby": "^9.0.0",
		"tslint": "^5.11.0",
		"typescript": "^2.6.1",
		"vscode": "^1.1.21"
	}
}